divide : Float -> Float -> ()
divide a b =
 do
   println! "dividir " a " en " b
   let result = try! a / b
   in case result of
     Exception result -> println! "something wrong happened"
     Success value -> println! "result is " value

main () = do

    println! $ divide 10 2

    println! $ divide 10 0

    divide 10 0