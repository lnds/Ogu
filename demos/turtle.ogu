from ogu.turtle import *

draw-fib! turtle depth =
   do
     move-forward! turtle 30
     if depth > 2 then
     do
       turn-left! turtle 15
       draw-fib! turtle (depth - 1)
       turn-right! turtle 30
       draw-fib! turtle (depth - 2)
       turn-left! turtle 15
       move-back! turtle 30
     else
       move-back! turtle 30

create-turtle! 400 400 !> pen-up! !> go-to! 0 -100 !> pen-down!  !> draw-fib! 10 !> show!