
greet0! = greet "you"

greet1! name
    | name == "world " = println! "hello world"
    | otherwise = println! "hello " name

greet2! name1 name2 = println! "hello " name1 " & " name2


main () = do

    greet0!

    greet1! "world"

    greet1! "Mampato"

    greet2! "Mampato" "Rena"

main () =
    greet2! "Mampato" "Ogu" "Rena" "Xse"
