
greet! = greet "you"

greet! name
    | name == "world " do println! "hello world"
    | otherwise do println! "hello " name

greet name1 name2 = println! "hello " name1 " & " name2


greet!

greet! "world"

greet! "Mampato"

greet! "Mampato" "Rena"


greet2! name rest... = println! $ str "hola " name ", " (join ", " rest)


greet2! "Mampato" "Ogu" "Rena" "Xse"
